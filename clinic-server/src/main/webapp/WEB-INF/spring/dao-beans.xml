<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.2.xsd">

	<bean name="countryMsDAO" class="com.neeraj.clinic.server.dao.CountryMsDAO">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>
	<bean name="stateMsDAO" class="com.neeraj.clinic.server.dao.StateMsDAO">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>
	<bean name="districtMsDAO" class="com.neeraj.clinic.server.dao.DistrictMsDAO">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>
	<bean name="areaMsDAO" class="com.neeraj.clinic.server.dao.AreaMsDAO">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>
	<bean name="blockMsDAO" class="com.neeraj.clinic.server.dao.BlockMsDAO">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>
	<bean name="storageAttribsMsDAO" class="com.neeraj.clinic.server.dao.StorageAttribsMsDAO">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>
	<bean name="brandMsDAO" class="com.neeraj.clinic.server.dao.BrandMsDAO">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>
	<bean name="medicineMsDAO" class="com.neeraj.clinic.server.dao.MedicineMsDAO">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>
	<bean name="inventoryDAO" class="com.neeraj.clinic.server.dao.InventoryDAO">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>
	<bean name="peopleMsDAO" class="com.neeraj.clinic.server.dao.PeopleMsDAO">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>
</beans>		
		
		
